{
  "name": "with-typescript-eslint-jest",
  "author": "@erikdstock",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write **/*.{js,ts,tsx}",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@hapi/iron": "^6.0.0",
    "@types/cookie": "^0.4.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.5.0",
    "cookie": "^0.4.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.22",
    "next": "9.4.4",
    "next-connect": "^0.8.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "swr": "^0.2.3",
    "zod": "^1.7.1"
  },
  "devDependencies": {
    "@testing-library/react": "^10.4.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^26.0.3",
    "@types/lodash": "^4.14.157",
    "@types/mongoose": "^5.7.30",
    "@types/node": "^14.0.14",
    "@types/passport-local": "^1.0.33",
    "@types/qs": "^6.9.3",
    "@types/react": "^16.9.41",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "babel-jest": "^26.1.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-watch-typeahead": "^0.6.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "typescript": "^3.9.6"
  }
}
